#include <iostream>
#include <math.h>
using namespace std;

int main()
{
	double a,b,c,d,r1,r2;
	
	cin>>a>>b>>c;
	
	d = ((b*b)-(4*a*c));
    r1 = ((-b+sqrt(d))/(2*a));
    r2 = ((-b-sqrt(d))/(2*a));
	
	if(a!=0 && d>0)
	{
		cout<<fixed;
		cout.precision(5);
		cout<<"R1 = "<<r1<<endl;
		cout<<"R2 = "<<r2<<endl;
	}
	else
		{
		cout<<"Impossivel calcular"<<endl;	
		}
	
}
